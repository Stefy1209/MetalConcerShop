<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="223.0" prefWidth="451.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LogInController">
   <children>
      <HBox alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="38.0" prefWidth="284.0" text="Welcome!" HBox.hgrow="ALWAYS">
               <font>
                  <Font name="System Bold" size="26.0" />
               </font>
            </Label>
            <VBox alignment="CENTER" prefHeight="263.0" prefWidth="299.0" spacing="5.0" HBox.hgrow="ALWAYS">
               <children>
                  <TextField fx:id="usernameTextField" prefHeight="25.0" prefWidth="172.0" promptText="Username" />
                  <PasswordField fx:id="passwordField" promptText="Password" />
                  <ComboBox fx:id="accountComboBox" prefHeight="25.0" prefWidth="232.0" promptText="Type Account" VBox.vgrow="ALWAYS" />
                  <HBox alignment="CENTER" prefHeight="17.0" prefWidth="223.0" spacing="5.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#logIn" prefHeight="25.0" prefWidth="118.0" text="Log In" HBox.hgrow="ALWAYS" />
                        <Button mnemonicParsing="false" onAction="#signUp" prefHeight="25.0" prefWidth="118.0" text="Sign Up" HBox.hgrow="ALWAYS" />
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
